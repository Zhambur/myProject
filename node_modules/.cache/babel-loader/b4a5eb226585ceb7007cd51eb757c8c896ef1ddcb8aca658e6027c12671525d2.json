{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      statCards: [{\n        title: \"总员工数\",\n        value: \"298\",\n        icon: \"el-icon-user-solid\",\n        bgColor: \"#409EFF\",\n        trend: {\n          type: \"steady\",\n          icon: \"el-icon-minus\",\n          value: \"稳定\"\n        }\n      }, {\n        title: \"登录事件\",\n        value: \"2,458\",\n        icon: \"el-icon-s-platform\",\n        bgColor: \"#67C23A\",\n        trend: {\n          type: \"up\",\n          icon: \"el-icon-top\",\n          value: \"+12.5%\"\n        }\n      }, {\n        title: \"网页访问\",\n        value: \"8,732\",\n        icon: \"el-icon-reading\",\n        bgColor: \"#E6A23C\",\n        trend: {\n          type: \"up\",\n          icon: \"el-icon-top\",\n          value: \"+8.3%\"\n        }\n      }, {\n        title: \"异常事件\",\n        value: \"23\",\n        icon: \"el-icon-warning-outline\",\n        bgColor: \"#F56C6C\",\n        trend: {\n          type: \"up\",\n          icon: \"el-icon-top\",\n          value: \"+5.7%\"\n        }\n      }],\n      activityType: \"login\",\n      activityChart: null,\n      threatChart: null,\n      departmentAbnormalChart: null,\n      threatFilter: \"all\",\n      recentEvents: [{\n        time: \"2017-11-23 18:45:12\",\n        type: \"数据泄露\",\n        description: \"员工1103在非工作时间向外部邮箱发送大量文件\",\n        level: \"高风险\"\n      }, {\n        time: \"2017-11-22 14:30:09\",\n        type: \"异常登录\",\n        description: \"员工1152多次尝试登录其他员工账户\",\n        level: \"中风险\"\n      }, {\n        time: \"2017-11-20 23:15:28\",\n        type: \"异常流量\",\n        description: \"员工1204与未知IP地址有加密通信\",\n        level: \"高风险\"\n      }, {\n        time: \"2017-11-19 06:22:45\",\n        type: \"非常规操作\",\n        description: \"员工1388在深夜时段频繁访问代码库\",\n        level: \"中风险\"\n      }, {\n        time: \"2017-11-15 15:10:33\",\n        type: \"数据库异常\",\n        description: \"员工1307执行大量非常规数据库查询操作\",\n        level: \"高风险\"\n      }]\n    };\n  },\n  mounted() {\n    this.initCharts();\n  },\n  methods: {\n    initCharts() {\n      this.$nextTick(() => {\n        this.initActivityChart();\n        this.initThreatChart();\n        this.initDepartmentAbnormalChart();\n      });\n    },\n    handleActivityTypeChange() {\n      this.initActivityChart();\n    },\n    handleThreatChartChange(command) {\n      this.threatFilter = command;\n      this.initThreatChart();\n    },\n    goToThreatPage() {\n      this.$router.push(\"/threat\");\n    },\n    initActivityChart() {\n      this.activityChart = this.$echarts.init(document.getElementById(\"activityChart\"));\n\n      // 生成X轴日期\n      const dates = [];\n      for (let i = 1; i <= 30; i++) {\n        dates.push(`2017-11-${i < 10 ? \"0\" + i : i}`);\n      }\n\n      // 根据活动类型生成不同的数据\n      let data = [];\n      let series = [];\n      if (this.activityType === \"login\") {\n        // 模拟登录数据\n        const normalLogin = dates.map(() => Math.floor(Math.random() * 40 + 60));\n        const failedLogin = dates.map(() => Math.floor(Math.random() * 10 + 5));\n        const abnormalLogin = dates.map(() => Math.floor(Math.random() * 3));\n\n        // 特意在某些日期制造异常数据\n        abnormalLogin[12] = 8;\n        abnormalLogin[20] = 12;\n        series = [{\n          name: \"正常登录\",\n          type: \"bar\",\n          stack: \"登录\",\n          emphasis: {\n            focus: \"series\"\n          },\n          data: normalLogin,\n          itemStyle: {\n            color: \"#67C23A\"\n          }\n        }, {\n          name: \"登录失败\",\n          type: \"bar\",\n          stack: \"登录\",\n          emphasis: {\n            focus: \"series\"\n          },\n          data: failedLogin,\n          itemStyle: {\n            color: \"#E6A23C\"\n          }\n        }, {\n          name: \"异常登录\",\n          type: \"bar\",\n          stack: \"登录\",\n          emphasis: {\n            focus: \"series\"\n          },\n          data: abnormalLogin,\n          itemStyle: {\n            color: \"#F56C6C\"\n          },\n          markPoint: {\n            data: [{\n              type: \"max\",\n              name: \"最大值\"\n            }]\n          }\n        }];\n      } else if (this.activityType === \"web\") {\n        // 模拟网页访问数据\n        const internal = dates.map(() => Math.floor(Math.random() * 200 + 150));\n        const external = dates.map(() => Math.floor(Math.random() * 100 + 50));\n\n        // 制造异常\n        external[15] = 280;\n        external[25] = 320;\n        series = [{\n          name: \"内部网站\",\n          type: \"line\",\n          smooth: true,\n          emphasis: {\n            focus: \"series\"\n          },\n          data: internal,\n          itemStyle: {\n            color: \"#409EFF\"\n          },\n          areaStyle: {\n            opacity: 0.3\n          }\n        }, {\n          name: \"外部网站\",\n          type: \"line\",\n          smooth: true,\n          emphasis: {\n            focus: \"series\"\n          },\n          data: external,\n          itemStyle: {\n            color: \"#E6A23C\"\n          },\n          areaStyle: {\n            opacity: 0.3\n          },\n          markPoint: {\n            data: [{\n              type: \"max\",\n              name: \"最大值\"\n            }]\n          },\n          markArea: {\n            data: [[{\n              name: \"异常时段\",\n              xAxis: \"2017-11-14\"\n            }, {\n              xAxis: \"2017-11-17\"\n            }], [{\n              name: \"异常时段\",\n              xAxis: \"2017-11-24\"\n            }, {\n              xAxis: \"2017-11-27\"\n            }]],\n            itemStyle: {\n              color: \"rgba(255, 0, 0, 0.1)\"\n            }\n          }\n        }];\n      } else if (this.activityType === \"email\") {\n        // 模拟邮件数据\n        const internal = dates.map(() => Math.floor(Math.random() * 80 + 120));\n        const external = dates.map(() => Math.floor(Math.random() * 30 + 20));\n\n        // 制造异常\n        external[8] = 85;\n        external[22] = 95;\n        series = [{\n          name: \"内部邮件\",\n          type: \"line\",\n          smooth: true,\n          emphasis: {\n            focus: \"series\"\n          },\n          data: internal,\n          symbolSize: 8,\n          itemStyle: {\n            color: \"#67C23A\"\n          }\n        }, {\n          name: \"外部邮件\",\n          type: \"line\",\n          smooth: true,\n          emphasis: {\n            focus: \"series\"\n          },\n          data: external,\n          symbolSize: 8,\n          itemStyle: {\n            color: \"#F56C6C\"\n          },\n          markPoint: {\n            data: [{\n              type: \"max\",\n              name: \"最大值\"\n            }, {\n              name: \"异常点\",\n              coord: [8, 85],\n              itemStyle: {\n                color: \"#F56C6C\"\n              },\n              label: {\n                formatter: \"异常外发\",\n                position: \"top\"\n              }\n            }, {\n              name: \"异常点\",\n              coord: [22, 95],\n              itemStyle: {\n                color: \"#F56C6C\"\n              },\n              label: {\n                formatter: \"异常外发\",\n                position: \"top\"\n              }\n            }]\n          }\n        }];\n      }\n      const option = {\n        tooltip: {\n          trigger: \"axis\",\n          axisPointer: {\n            type: \"shadow\"\n          }\n        },\n        legend: {\n          top: 10,\n          data: series.map(item => item.name)\n        },\n        grid: {\n          left: \"3%\",\n          right: \"4%\",\n          bottom: \"3%\",\n          containLabel: true\n        },\n        xAxis: [{\n          type: \"category\",\n          boundaryGap: true,\n          data: dates,\n          axisLabel: {\n            formatter: function (value) {\n              return value.substring(5); // 只显示月-日\n            },\n            rotate: 45,\n            interval: \"auto\"\n          }\n        }],\n        yAxis: [{\n          type: \"value\",\n          name: \"数量\",\n          axisLabel: {\n            formatter: \"{value}\"\n          }\n        }],\n        dataZoom: [{\n          type: \"inside\",\n          start: 0,\n          end: 100\n        }, {\n          start: 0,\n          end: 100\n        }],\n        series: series\n      };\n      this.activityChart.setOption(option);\n    },\n    initThreatChart() {\n      this.threatChart = this.$echarts.init(document.getElementById(\"threatChart\"));\n\n      // 根据筛选条件获取数据\n      let data = [];\n      if (this.threatFilter === \"all\" || this.threatFilter === \"high\") {\n        data.push({\n          value: 8,\n          name: \"数据泄露\",\n          itemStyle: {\n            color: \"#F56C6C\"\n          }\n        }, {\n          value: 5,\n          name: \"异常通信\",\n          itemStyle: {\n            color: \"#F56C6C\"\n          }\n        });\n      }\n      if (this.threatFilter === \"all\" || this.threatFilter === \"medium\") {\n        data.push({\n          value: 6,\n          name: \"异常登录\",\n          itemStyle: {\n            color: \"#E6A23C\"\n          }\n        }, {\n          value: 4,\n          name: \"非常规操作\",\n          itemStyle: {\n            color: \"#E6A23C\"\n          }\n        });\n      }\n      if (this.threatFilter === \"all\" || this.threatFilter === \"low\") {\n        data.push({\n          value: 3,\n          name: \"审计失败\",\n          itemStyle: {\n            color: \"#909399\"\n          }\n        }, {\n          value: 2,\n          name: \"权限异常\",\n          itemStyle: {\n            color: \"#909399\"\n          }\n        });\n      }\n      const option = {\n        tooltip: {\n          trigger: \"item\",\n          formatter: \"{a} <br/>{b}: {c} ({d}%)\"\n        },\n        legend: {\n          orient: \"vertical\",\n          right: 10,\n          top: \"center\",\n          data: data.map(item => item.name)\n        },\n        series: [{\n          name: \"威胁类型\",\n          type: \"pie\",\n          radius: [\"40%\", \"70%\"],\n          center: [\"40%\", \"50%\"],\n          avoidLabelOverlap: false,\n          emphasis: {\n            itemStyle: {\n              shadowBlur: 10,\n              shadowOffsetX: 0,\n              shadowColor: \"rgba(0, 0, 0, 0.5)\"\n            }\n          },\n          label: {\n            show: false,\n            position: \"center\"\n          },\n          labelLine: {\n            show: false\n          },\n          data: data\n        }]\n      };\n      this.threatChart.setOption(option);\n\n      // 添加点击事件\n      this.threatChart.on(\"click\", params => {\n        this.$notify({\n          title: \"威胁详情\",\n          message: `${params.name}类型威胁共有${params.value}个事件，点击查看详情`,\n          type: \"warning\",\n          duration: 3000\n        });\n      });\n    },\n    initDepartmentAbnormalChart() {\n      this.departmentAbnormalChart = this.$echarts.init(document.getElementById(\"departmentAbnormalChart\"));\n      const option = {\n        tooltip: {\n          trigger: \"axis\",\n          axisPointer: {\n            type: \"shadow\"\n          }\n        },\n        legend: {\n          data: [\"高风险\", \"中风险\", \"低风险\"]\n        },\n        grid: {\n          left: \"3%\",\n          right: \"4%\",\n          bottom: \"3%\",\n          containLabel: true\n        },\n        xAxis: {\n          type: \"value\",\n          boundaryGap: [0, 0.01]\n        },\n        yAxis: {\n          type: \"category\",\n          data: [\"财务部\", \"人力资源部\", \"研发3部\", \"研发2部\", \"研发1部\"]\n        },\n        series: [{\n          name: \"高风险\",\n          type: \"bar\",\n          data: [2, 1, 3, 1, 0],\n          itemStyle: {\n            color: \"#F56C6C\"\n          }\n        }, {\n          name: \"中风险\",\n          type: \"bar\",\n          data: [1, 2, 2, 3, 1],\n          itemStyle: {\n            color: \"#E6A23C\"\n          }\n        }, {\n          name: \"低风险\",\n          type: \"bar\",\n          data: [1, 2, 1, 2, 1],\n          itemStyle: {\n            color: \"#909399\"\n          }\n        }]\n      };\n      this.departmentAbnormalChart.setOption(option);\n    },\n    handleEventClick(row) {\n      this.$router.push(\"/threat\");\n    },\n    getEventRowClass({\n      row\n    }) {\n      if (row.level === \"高风险\") {\n        return \"high-risk-row\";\n      }\n      return \"\";\n    },\n    getEventTypeTag(type) {\n      const typeMap = {\n        数据泄露: \"danger\",\n        异常登录: \"warning\",\n        异常流量: \"danger\",\n        非常规操作: \"warning\",\n        数据库异常: \"danger\"\n      };\n      return typeMap[type] || \"info\";\n    },\n    getRiskLevelTag(level) {\n      const levelMap = {\n        高风险: \"danger\",\n        中风险: \"warning\",\n        低风险: \"info\"\n      };\n      return levelMap[level] || \"info\";\n    }\n  },\n  beforeDestroy() {\n    // 销毁图表实例\n    this.activityChart && this.activityChart.dispose();\n    this.threatChart && this.threatChart.dispose();\n    this.departmentAbnormalChart && this.departmentAbnormalChart.dispose();\n  },\n  watch: {\n    // 监听窗口大小变化，调整图表大小\n    \"$store.state.app.sidebar.opened\"() {\n      this.$nextTick(() => {\n        this.activityChart && this.activityChart.resize();\n        this.threatChart && this.threatChart.resize();\n        this.departmentAbnormalChart && this.departmentAbnormalChart.resize();\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","statCards","title","value","icon","bgColor","trend","type","activityType","activityChart","threatChart","departmentAbnormalChart","threatFilter","recentEvents","time","description","level","mounted","initCharts","methods","$nextTick","initActivityChart","initThreatChart","initDepartmentAbnormalChart","handleActivityTypeChange","handleThreatChartChange","command","goToThreatPage","$router","push","$echarts","init","document","getElementById","dates","i","series","normalLogin","map","Math","floor","random","failedLogin","abnormalLogin","stack","emphasis","focus","itemStyle","color","markPoint","internal","external","smooth","areaStyle","opacity","markArea","xAxis","symbolSize","coord","label","formatter","position","option","tooltip","trigger","axisPointer","legend","top","item","grid","left","right","bottom","containLabel","boundaryGap","axisLabel","substring","rotate","interval","yAxis","dataZoom","start","end","setOption","orient","radius","center","avoidLabelOverlap","shadowBlur","shadowOffsetX","shadowColor","show","labelLine","on","params","$notify","message","duration","handleEventClick","row","getEventRowClass","getEventTypeTag","typeMap","数据泄露","异常登录","异常流量","非常规操作","数据库异常","getRiskLevelTag","levelMap","高风险","中风险","低风险","beforeDestroy","dispose","watch","$store.state.app.sidebar.opened","resize"],"sources":["src/views/Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <div class=\"dashboard-header\">\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"6\" v-for=\"(card, index) in statCards\" :key=\"index\">\r\n          <el-card shadow=\"hover\" class=\"stat-card\">\r\n            <div class=\"stat-card-content\">\r\n              <div\r\n                class=\"stat-card-icon\"\r\n                :style=\"{ backgroundColor: card.bgColor }\"\r\n              >\r\n                <i :class=\"card.icon\"></i>\r\n              </div>\r\n              <div class=\"stat-card-info\">\r\n                <div class=\"stat-card-title\">{{ card.title }}</div>\r\n                <div class=\"stat-card-value\">{{ card.value }}</div>\r\n                <div class=\"stat-card-trend\" :class=\"card.trend.type\">\r\n                  <i :class=\"card.trend.icon\"></i>\r\n                  <span>{{ card.trend.value }}</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n\r\n    <el-row :gutter=\"20\" class=\"main-charts\">\r\n      <el-col :span=\"16\">\r\n        <el-card shadow=\"hover\" class=\"chart-card\">\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span>系统活动总览</span>\r\n            <el-radio-group\r\n              v-model=\"activityType\"\r\n              size=\"mini\"\r\n              @change=\"handleActivityTypeChange\"\r\n              style=\"float: right\"\r\n            >\r\n              <el-radio-button label=\"login\">登录活动</el-radio-button>\r\n              <el-radio-button label=\"web\">网页访问</el-radio-button>\r\n              <el-radio-button label=\"email\">邮件活动</el-radio-button>\r\n            </el-radio-group>\r\n          </div>\r\n          <div id=\"activityChart\" style=\"width: 100%; height: 400px\"></div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <el-col :span=\"8\">\r\n        <el-card shadow=\"hover\" class=\"chart-card\">\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span>威胁分布</span>\r\n            <el-dropdown\r\n              trigger=\"click\"\r\n              @command=\"handleThreatChartChange\"\r\n              style=\"float: right; cursor: pointer\"\r\n            >\r\n              <span class=\"el-dropdown-link\">\r\n                筛选 <i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n              </span>\r\n              <el-dropdown-menu slot=\"dropdown\">\r\n                <el-dropdown-item command=\"all\">全部</el-dropdown-item>\r\n                <el-dropdown-item command=\"high\">高风险</el-dropdown-item>\r\n                <el-dropdown-item command=\"medium\">中风险</el-dropdown-item>\r\n                <el-dropdown-item command=\"low\">低风险</el-dropdown-item>\r\n              </el-dropdown-menu>\r\n            </el-dropdown>\r\n          </div>\r\n          <div id=\"threatChart\" style=\"width: 100%; height: 400px\"></div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\" class=\"detail-charts\">\r\n      <el-col :span=\"12\">\r\n        <el-card shadow=\"hover\" class=\"chart-card\">\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span>部门异常活动统计</span>\r\n          </div>\r\n          <div\r\n            id=\"departmentAbnormalChart\"\r\n            style=\"width: 100%; height: 320px\"\r\n          ></div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <el-col :span=\"12\">\r\n        <el-card shadow=\"hover\" class=\"chart-card\">\r\n          <div slot=\"header\" class=\"clearfix\">\r\n            <span>最近异常事件</span>\r\n            <el-button type=\"text\" style=\"float: right\" @click=\"goToThreatPage\"\r\n              >查看全部</el-button\r\n            >\r\n          </div>\r\n          <el-table\r\n            :data=\"recentEvents\"\r\n            style=\"width: 100%\"\r\n            :row-class-name=\"getEventRowClass\"\r\n            @row-click=\"handleEventClick\"\r\n          >\r\n            <el-table-column\r\n              prop=\"time\"\r\n              label=\"时间\"\r\n              width=\"180\"\r\n            ></el-table-column>\r\n            <el-table-column prop=\"type\" label=\"类型\" width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag :type=\"getEventTypeTag(scope.row.type)\">{{\r\n                  scope.row.type\r\n                }}</el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"description\" label=\"描述\"></el-table-column>\r\n            <el-table-column prop=\"level\" label=\"风险等级\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag :type=\"getRiskLevelTag(scope.row.level)\" size=\"mini\">\r\n                  {{ scope.row.level }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Home\",\r\n  data() {\r\n    return {\r\n      statCards: [\r\n        {\r\n          title: \"总员工数\",\r\n          value: \"298\",\r\n          icon: \"el-icon-user-solid\",\r\n          bgColor: \"#409EFF\",\r\n          trend: {\r\n            type: \"steady\",\r\n            icon: \"el-icon-minus\",\r\n            value: \"稳定\",\r\n          },\r\n        },\r\n        {\r\n          title: \"登录事件\",\r\n          value: \"2,458\",\r\n          icon: \"el-icon-s-platform\",\r\n          bgColor: \"#67C23A\",\r\n          trend: {\r\n            type: \"up\",\r\n            icon: \"el-icon-top\",\r\n            value: \"+12.5%\",\r\n          },\r\n        },\r\n        {\r\n          title: \"网页访问\",\r\n          value: \"8,732\",\r\n          icon: \"el-icon-reading\",\r\n          bgColor: \"#E6A23C\",\r\n          trend: {\r\n            type: \"up\",\r\n            icon: \"el-icon-top\",\r\n            value: \"+8.3%\",\r\n          },\r\n        },\r\n        {\r\n          title: \"异常事件\",\r\n          value: \"23\",\r\n          icon: \"el-icon-warning-outline\",\r\n          bgColor: \"#F56C6C\",\r\n          trend: {\r\n            type: \"up\",\r\n            icon: \"el-icon-top\",\r\n            value: \"+5.7%\",\r\n          },\r\n        },\r\n      ],\r\n      activityType: \"login\",\r\n      activityChart: null,\r\n      threatChart: null,\r\n      departmentAbnormalChart: null,\r\n      threatFilter: \"all\",\r\n      recentEvents: [\r\n        {\r\n          time: \"2017-11-23 18:45:12\",\r\n          type: \"数据泄露\",\r\n          description: \"员工1103在非工作时间向外部邮箱发送大量文件\",\r\n          level: \"高风险\",\r\n        },\r\n        {\r\n          time: \"2017-11-22 14:30:09\",\r\n          type: \"异常登录\",\r\n          description: \"员工1152多次尝试登录其他员工账户\",\r\n          level: \"中风险\",\r\n        